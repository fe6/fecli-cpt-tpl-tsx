{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "react",
    "es2015"
  ],
  "plugins": [
    "transform-runtime",
    ["transform-strict-mode", {
      "strict": false
    }],
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
